<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="eBay API Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="Saucelabs"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="chrome"/>
    <parameter name="browserVersion" value="60.0"/>
    <parameter name="url" value="https://www.eBay.com/"/>


<test name="search Items">
    <parameter name="browserName" value="chrome"/>

    <classes>

        <class name="search.TestSearch"/>
        <class name="search.DailyDealsTest"/>
        <class name="search.GiftCardsTest"/>
        <class name="search.HomePageTest"/>
        <class name="search.InputHome"/>
        <class name="search.SignInPageTest"/>
        <class name="search.SignUpPageTest"/>
        <class name="search.HelpAndContactTest"/>
        <class name="databaseTest.DBSearchTest"/>
        <class name="dataDrivenTest.InputFromExcelTest"/>
        <class name="googleSheetTest.TestGoogleSheetsPage"/>
        <class name="keyWordDrivenTest.TestFeatures"/>

    </classes>
</test>


</suite>
