<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Facebook API Automation Script " parallel="false">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="OS X"/>
    <parameter name="os_version" value="Sierra"/>
    <parameter name="browserName" value="Firefox"/>
    <parameter name="browserVersion" value="62.0"/>
    <parameter name="url" value="https://www.facebook.com"/>


    <test name="Search Items">
        <parameter name="browserName" value="Firefox"/>

        <classes>

            <class name="HomePageFunctionTest.homepagetest"/>

            <class name="LoginPackageTest.loginpagetest"/>

        </classes>
    </test>


</suite>